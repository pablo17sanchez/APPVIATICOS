@model Model.Municipio

@{
    ViewBag.Title = "Editar Municipios";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="row">

    <div class="col-lg-12">
        <h3 class="page-header"><i class="fa fa-files-o"></i>Formulario Municipio</h3>
        <ol class="breadcrumb">
            <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
            <li><i class="icon_document_alt"></i>Form</li>
            <li><i class="fa fa-files-o"></i>Municipio</li>
        </ol>
    </div>
</div>







<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Formulario  de edicion de municipios
            </header>
            <div class="panel-body">
                <div class="form">


                    @using (Html.BeginForm())
                    {
                        @Html.AntiForgeryToken()

                        <div class="form-horizontal">
                            <h4>Municipio</h4>
                            <hr />
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                            @Html.HiddenFor(model => model.id)

                            <div class="form-group">
                                <label for="Cargo" class="control-label col-lg-2">Codigo de Region<span class="required">*</span></label>
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.COD_REGION, new { htmlAttributes = new { @class = "form-control", @type = "number" } })
                                    @Html.ValidationMessageFor(model => model.COD_REGION, "", new { @class = "text-danger" })
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="Cargo" class="control-label col-lg-2">Codigo de Provincia<span class="required">*</span></label>
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.COD_PROVINCIA, new { htmlAttributes = new { @class = "form-control", @type = "number" } })
                                    @Html.ValidationMessageFor(model => model.COD_PROVINCIA, "", new { @class = "text-danger" })
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="Cargo" class="control-label col-lg-2">Codigo de Municipio<span class="required">*</span></label>
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.COD_MUNICIPIO, new { htmlAttributes = new { @class = "form-control", @type = "number" } })
                                    @Html.ValidationMessageFor(model => model.COD_MUNICIPIO, "", new { @class = "text-danger" })
                                </div>
                            </div>

                            <div class="form-group">
                                <label for="Cargo" class="control-label col-lg-2">Nombre del Municipio<span class="required">*</span></label>
                                <div class="col-md-10">
                                    @Html.EditorFor(model => model.NOM_MUNICIPIO, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.NOM_MUNICIPIO, "", new { @class = "text-danger" })
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Save" class="btn btn-default" />
                                </div>
                            </div>
                        </div>
                    }
                    <div>
                        @*@Html.ActionLink("Back to List", "Index")*@
                    </div>
                </div>

            </div>
        </section>
    </div>
</div>
